{
  "name": "mojito-cli-build",
  "version": "0.0.1",
  "description": "Subcommand for mojito-cli to make client-side mojito apps.",
  "main": "build.js",
  "dependencies": {
    "mkdirp": "~0.3.5",
    "npmlog": "0.0.2",
    "rimraf": "~2.1.4",
    "wrench": "~1.4.4"
  },
  "devDependencies": {
    "tap": "~0.4.1"
  },
  "engines": {
    "node": ">0.8",
    "npm": ">1.1"
  },
  "scripts": {
    "prepublish": "npm test",
    "pretest": "jshint {.,lib}/*.js*",
    "test": "tap tests/index.js",
    "cover": "istanbul cover --dir artifacts/coverage tests",
    "clean": "git clean -dfX **/artifacts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yahoo/mojito-cli-build.git"
  },
  "contributors": [
    "Caridy Patino <caridy@gmail.com>",
